{
  "name": "baus-boilerplate",
  "version": "1.0.1",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "npm run clean && NODE_ENV=development nodemon server/index.js --watch server",
    "postinstall": "npm run dist",
    "dist": "npm run clean && npm run webpack-prod",
    "clean": "rm -rf public/dist && mkdir public/dist",
    "webpack-prod": "webpack --config ./webpack/production.config.js --progress --colors",
