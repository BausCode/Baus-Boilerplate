{
  "presets": [
    "react",
    "es2015", 
    "stage-0"
  ],

  "plugins": [
    "syntax-decorators"
  ],

  "env": {
    "development": {
      "plugins": [
        ["react-transform", {
          "transforms": [{
            "transform": "react-transform-catch-errors",
            "imports": [
              "react", 
              "redbox-react"
            ]
          }]
        }]
      ]
    }
  }
}
